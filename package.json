  "main": "index.js",
  "bin": {
    "run": "./bin/run.js"
  },
  "scripts": {
    "index": "node ./bin/script/makeCommonIndex",
    "main": "node ./bin/run.js"
  },
  "keywords": [
    "style-component-compiler",
    "style-unit-compiler",
    "style-component",
    "style-components",
    "style-lib",
    "style-library"
  ],
  "author": "xiehaihong",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^11.1.1",
    "progress": "^2.0.3",
    "sass": "^1.69.4"
  }
}
